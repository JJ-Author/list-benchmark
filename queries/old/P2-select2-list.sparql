prefix midi: <http://purl.org/midi-ld/midi#>
prefix midi-note: <http://purl.org/midi-ld/notes/>
prefix midi-prog: <http://purl.org/midi-ld/programs/>
prefix prov: <http://www.w3.org/ns/prov#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix xml: <http://www.w3.org/XML/1998/namespace>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>

SELECT
	?track
	?event
	?attribute
	?value
FROM <%%DATASET%%>
WHERE {
   ?track a midi:Track .
   ?track midi:hasEvents ?events .
   ?events rdf:rest*/rdf:first ?event .
   ?event ?attribute ?value .
	 BIND (xsd:int(SUBSTR(str(?event), 77)) AS ?id)
} ORDER BY ?id
LIMIT %LIMIT% # Set to 1 for random access
OFFSET %OFFSET%
