prefix midi: <http://purl.org/midi-ld/midi#>
prefix midi-note: <http://purl.org/midi-ld/notes/>
prefix midi-prog: <http://purl.org/midi-ld/programs/>
prefix prov: <http://www.w3.org/ns/prov#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix xml: <http://www.w3.org/XML/1998/namespace>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
prefix sequence: <http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#>

WITH <%%DATASET%%>
INSERT
	{ <%%TRACK%%>
		midi:hasEvent <http://example.org/appended-event-SOP> .
		<http://example.org/appended-event-SOP> sequence:follows ?event .
		?event sequence:precedes <http://example.org/appended-event-SOP> .
} WHERE	{  
		<%%TRACK%%> midi:hasEvent ?event .
		FILTER NOT EXISTS {?event sequence:precedes []}
}
