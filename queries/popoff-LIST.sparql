prefix midi: <http://purl.org/midi-ld/midi#>
prefix midi-note: <http://purl.org/midi-ld/notes/>
prefix midi-prog: <http://purl.org/midi-ld/programs/>
prefix prov: <http://www.w3.org/ns/prov#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix xml: <http://www.w3.org/XML/1998/namespace>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>

WITH <%%DATASET%%>
DELETE {
	<%%TRACK%%> midi:hasEvents ?events .
	?events a rdf:List ;
			rdf:first ?first ;
			rdf:rest ?rest .
}
INSERT
	{ 
		<%%TRACK%%> midi:hasEvents ?rest
} WHERE	{  
		<%%TRACK%%> midi:hasEvents ?events .
		?events
			rdf:first ?first ;
			rdf:rest ?rest.
}
# DELETE THE FIRST LIST ELEMENT